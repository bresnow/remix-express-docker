{
  "name": "remix-express-docker",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.ts --outdir=./server",
    "dev": "remix build && run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./server/index.js --watch ./server/index.js",
    "dev:remix": "remix watch",
    "watch": "run-p watch:*",
    "watch:remix": "remix watch",
    "watch:server": "esbuild --platform=node --format=cjs ./server/index.ts --outdir=./server --watch",
    "start": "cross-env NODE_ENV=production node ./server/index.js"
  },
  "dependencies": {
    "@remix-run/express": "1.4.3",
    "@remix-run/node": "1.4.3",
    "@remix-run/react": "1.4.3",
    "compression": "1.7.4",
    "express": "4.18.1",
    "livereload": "^0.9.3",
    "morgan": "1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.4.3",
    "@remix-run/eslint-config": "1.4.3",
    "@remix-run/serve": "1.4.3",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "cross-env": "7.0.3",
    "esbuild": "0.14.39",
    "eslint": "8.15.0",
    "nodemon": "2.0.16",
    "npm-run-all": "4.1.5",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
